  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dialogflow": "^1.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2"
  }

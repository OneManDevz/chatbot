  "dependencies": {
    "@material-ui/core": "^4.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "config": "^3.2.0",
    "cors": "^2.8.4",
    "dialogflow": "^0.12.0",
    "express": "^4.16.3",
    "mongoose": "^5.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.17.5",
    "prettier": "^1.18.2"
  }
